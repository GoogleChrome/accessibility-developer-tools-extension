{
  "name": "Accessibility Developer Tools (Chrome WebUI)",
  "version": "2.4",
  "description": "Accessibility audit and element properties.",
  "default_locale": "en",
  "icons": {
      "128": "images/icon128.png"
  },
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "experimental",
    "tabs",
    "webNavigation",
    "<all_urls>",
    "chrome://*/*"
  ],
  "web_accessible_resources": [
    "generated/axs.js",
    "generated/constants.js",
    "generated/extension.js",
    "generated/utils.js",
    "generated/properties.js",
    "generated/audits.js",
    "devtools.js",
    "devtools.html",
    "sidebar.html",
    "sidebar.js",
    "sidebar.css"
  ],
  "manifest_version": 2,
  "minimum_chrome_version": "21.0.1137.1"
}
